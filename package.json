{
  "name": "mysite-mahjong",
  "version": "1.0.0",
  "description": "Mahjong solitaire",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run styles:dev:watch\" \"npm run build:dev:watch\" \"npm run serve\"",
    "serve": "lite-server -c=bs-config.json",
    "prod": "npm-run-all clean styles:prod build:prod",
    "build:prod": "webpack --mode production",
    "build:dev:watch": "webpack --watch",
    "clean": "rm -f src/*.svg src/*.woff2 src/*.woff src/*.eot src/*.ttf",
    "styles:prod": "sass --no-source-map src/styles:src/styles",
    "styles:dev:watch": "sass --watch src/styles:src/styles",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "tsc": "tsc -p src/",
    "tsc:watch": "tsc -p src/ -w",
    "tsc:e2e": "tsc -p e2e/",
    "pree2e": "npm run tsc:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run tsc",
    "test": "concurrently \"npm run tsc:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run tsc",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@types/gsap": "^1.19.1",
    "@vercel/analytics": "^1.3.1",
    "angular-in-memory-web-api": "~0.2.4",
    "browser-sync-client": "^3.0.2",
    "core-js": "^2.4.1",
    "createjs": "^1.0.1",
    "createjs-module": "^0.8.3",
    "createjs-preloadjs": "0.6.2",
    "createjs-soundjs": "0.6.2",
    "csshake": "1.5.1",
    "font-awesome": "^4.7.0",
    "gsap": "^1.19.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@types/cors": "^2.8.17",
    "@types/jasmine": "2.5.36",
    "@types/node": "^22.3.0",
    "babel-loader": "^9.1.3",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "jasmine-core": "~2.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.5.4",
    "rxjs": "^5.5.12",
    "sass": "^1.78.0",
    "style-loader": "^4.0.0",
    "tslint": "^3.15.1",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {}
}
